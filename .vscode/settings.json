{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "*.inc": "cpp",
    "chrono": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "complex": "cpp",
    "exception": "cpp",
    "functional": "cpp",
    "algorithm": "cpp",
    "random": "cpp",
    "fstream": "cpp",
    "future": "cpp",
    "istream": "cpp",
    "memory": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "numeric": "cpp",
    "sstream": "cpp",
    "streambuf": "cpp",
    "string_view": "cpp",
    "cmath": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "utility": "cpp"
  }
}
